    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Model Demo",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/model_demo",
            "program": "demo.py",
            "console": "integratedTerminal",
        },
        {
            "name": "Python Debugger: Prepare Dataset",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/yolo",
            "program": "create_dataset.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Train YOLO",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/yolo",
            "program": "train_yolo.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Run YOLO",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/yolo",
            "program": "run_yolo.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Tracking",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/tracking",
            "program": "tracking.py",
            "console": "integratedTerminal"
        }
    ]
}